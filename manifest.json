{
   "content_scripts": [ {
      "css": [ "src/styles.css" ],
      "js": [ "src/script.js" ],
      "matches": [ "*://keep.google.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Compact list view with scrollable items for Google Keep powerusers",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "Google Keep Compact - Lists for Powerusers",
   "permissions": [ "*://keep.google.com/*" ],
   "version": "1.0.2"
}
